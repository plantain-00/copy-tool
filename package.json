{
  "name": "copy-tool",
  "version": "1.0.0",
  "description": "A tool to copy text or file from one place, and get it from another place",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "build-static": "tsc -p static && npm run minifier-template && webpack --display-modules",
    "build-static-debug": "tsc -p static && npm run minifier-template-debug && webpack --display-modules --config webpack.config.debug.js",
    "lint": "tslint index.ts \"static/*.ts\"",
    "minifier-template": "html-minifier --collapse-whitespace --case-sensitive static/app.template.html -o static/app.html",
    "minifier-template-debug": "html-minifier --case-sensitive static/app.template.html -o static/app.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/copy-tool.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/plantain-00/copy-tool/issues"
  },
  "homepage": "https://github.com/plantain-00/copy-tool#readme",
  "dependencies": {
    "express": "^4.14.0",
    "minimist": "^1.2.0",
    "socket.io": "^1.4.8"
  },
  "devDependencies": {
    "@angular/common": "~2.0.1",
    "@angular/compiler": "~2.0.1",
    "@angular/core": "~2.0.1",
    "@angular/forms": "~2.0.1",
    "@angular/http": "~2.0.1",
    "@angular/platform-browser": "~2.0.1",
    "@angular/platform-browser-dynamic": "~2.0.1",
    "@angular/router": "~3.0.1",
    "@angular/upgrade": "~2.0.1",
    "@types/core-js": "^0.9.34",
    "@types/express": "^4.0.33",
    "@types/minimist": "^1.1.29",
    "@types/socket.io": "^1.4.27",
    "@types/socket.io-client": "^1.4.27",
    "core-js": "^2.4.1",
    "html-minifier": "^3.1.0",
    "raw-loader": "^0.5.1",
    "rxjs": "5.0.0-beta.12",
    "socket.io-client": "^1.4.8",
    "tslib": "^1.0.0",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3",
    "webpack": "^1.13.2",
    "zone.js": "^0.6.25"
  }
}