{
  "name": "copy-tool",
  "version": "1.0.1",
  "description": "A tool to copy text or file from one place, and get it from another place",
  "main": "index.js",
  "scripts": {
    "bundle-vendor-css": "cleancss ./node_modules/bootstrap/dist/css/bootstrap.min.css ./node_modules/github-fork-ribbon-css/gh-fork-ribbon.css -o static/vendor.bundle.css",
    "bundle-index-css": "cleancss static/index.css -o static/index.bundle.css",
    "clean-rev": "rimraf static/vendor.bundle-*.js static/vendor.bundle-*.css static/index.bundle-*.js static/index.bundle-*.css",
    "rev": "npm run clean-rev && rev-static",
    "build": "tsc && npm run bundle-vendor-css && npm run bundle-index-css && tsc -p static && npm run minifier-template && webpack --display-modules && webpack --display-modules --config webpack.config.worker.js && npm run rev && webpack --display-modules --config webpack.config.service-worker.js",
    "lint": "tslint index.ts \"static/*.ts\"",
    "minifier-template": "html-minifier --collapse-whitespace --case-sensitive static/app.template.html -o static/app.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/copy-tool.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/copy-tool/issues"
  },
  "homepage": "https://github.com/plantain-00/copy-tool#readme",
  "dependencies": {
    "express": "4.15.2",
    "lodash.debounce": "4.0.8",
    "minimist": "1.2.0",
    "socket.io": "1.7.3"
  },
  "devDependencies": {
    "@types/clipboard": "1.5.32",
    "@types/express": "4.0.35",
    "@types/minimist": "1.2.0",
    "@types/qrcode": "0.0.2",
    "@types/socket.io": "1.4.29",
    "@types/socket.io-client": "1.4.29",
    "bootstrap": "3.3.7",
    "clean-css-cli": "4.0.10",
    "clipboard": "1.6.1",
    "file-uploader-component": "1.1.4",
    "github-fork-ribbon-css": "0.2.1",
    "html-minifier": "3.4.2",
    "js-split-file": "1.1.6",
    "qrcode": "0.8.0",
    "raw-loader": "0.5.1",
    "rev-static": "2.1.2",
    "rimraf": "2.6.1",
    "socket.io-client": "1.7.3",
    "tslib": "1.6.0",
    "tslint": "5.0.0",
    "typescript": "2.2.2",
    "vue": "2.2.6",
    "vue-class-component": "5.0.1",
    "webpack": "2.3.2",
    "webrtc-adapter": "3.2.0"
  }
}
